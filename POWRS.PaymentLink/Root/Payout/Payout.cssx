.title1
{
 font-family: "RT Dromo"; 
 font-size: Large;
}

.item {
  width: 100%;
  height: 71px;
  background: rgba(246,246,248,1);
  opacity: 1;
  position: relative;
  top: 0px;
  left: 0px;
  padding : 10px;
  border-top-left-radius: 11px;
  border-top-right-radius: 11px;
  border-bottom-left-radius: 11px;
  border-bottom-right-radius: 11px;
  overflow: hidden;
}

.total{
  width: 100%;
  height: 71px;
  background: rgba(246,246,248,1);
  opacity: 1;
  position: relative;
  top: 0px;
  left: 0px;
  padding : 10px;
  border-top-left-radius: 11px;
  border-top-right-radius: 11px;
  border-bottom-left-radius: 11px;
  border-bottom-right-radius: 11px;
  overflow: hidden;
}

.itemPrice {
  width:20%; 
  text-align:right;
  rowspan:"2";
  vertical-align:middle;
}

.price
{
  width: auto;
  height: auto;
  background:  rgba(246,246,248,1);
  opacity: 1;
  overflow: hidden;
  top: 0px;
  left: 0px;
  padding : 5px;
  border: 0.5px solid rgba(129,138,153,1);
  border-top-left-radius: 7px;
  border-top-right-radius: 7px;
  border-bottom-left-radius: 7px;
  border-bottom-right-radius: 7px;
}
.spaceItem
{
  height:10px;
}

.divSubmit
{
  width:100%; 
  display: flex; 
  align-items: center; 
  justify-content: center;
}



.btnSubmit{
  width: 90%;
  height: 71px;
  background: url("vaulterlogo.svg");
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  border: 1px solid rgba(187,195,206,1);
  border-top-left-radius: 11px;
  border-top-right-radius: 11px;
  border-bottom-left-radius: 11px;
  border-bottom-right-radius: 11px;
  overflow: hidden;
}

.name-balance{
  width: 100%;
}
.account-item {
  color: black;
  display:flex;
  align-items: center;
  text-align: end;
  margin-bottom: 10px;
  background: #f3f3f3;
  border: 2px solid #f3f3f3;
  border-radius: 10px;
  padding: 5px;
  width: 100%;
}
.account-item:active, .account-item:hover{
   border: 2px solid white;
   background: white;
}

.account-logo {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  margin-right: 10px;
}

.account-name {
  text-align: start;
  font-size: 16px;
  font-weight: bold;
}
