nav
{
    padding:0 3em 0 0;
}

nav > ul 
{
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
	display:flex;
}

nav > ul > li
{
    float: left;
    background-clip: content-box;
	white-space:nowrap;
	transition: opacity 0.25s ease-out;
	color:#b0b0b0;
    font-weight: 600;
	opacity:1;
}

header:hover > nav > ul > li
{
	opacity:1;
}

nav > ul > li:nth-last-of-type(1)
{
    float: right;
	order:4;
}

nav > ul > li:nth-last-of-type(2)
{
    float: right;
	order:5;
}

nav > ul > li:nth-last-of-type(3)
{
    float: right;
	order:6;
}

nav > ul > li:nth-last-of-type(4)
{
	text-align:center;
    display: block;
    padding: 0.9em 1em;
	flex-basis:100%;
	display:inline-block;
	font-weight:bold;
	opacity:1;
	border-left:0;
    color: #fff;
}

nav > ul > li:nth-last-of-type(5)
{
    
}

nav > ul > li:first-child
{
    border-left: 0;
}

nav > ul > li > a
{
    display: block;
    text-align: center;
    padding: 0.9em 1em;
    text-decoration: none;
    color: ¤MenuTextColor¤;
}

nav > ul > li > a:hover
{ 
    color: #FFF;
    font-weight:600;
}

