font-weight: normal;
font-style: normal;
}

@font-face {
font-family: 'RTDromo Medium Oblique';
src: url('../css/Fonts/RTDromo-MediumOblique.woff2') format('woff2'),
url('fonts/RTDromo-MediumOblique.woff') format('woff');
font-weight: normal;
font-style: normal;
}


 .qr-timer {
        display: flex;
        align-items: center;
        flex-direction: column;
        margin: 0;
        h1 {
          color: #111;
          font-size: 21px;
          margin-top: 1rem;
        }
        svg {
          height: 128px;
          width: 128px;
        }
        .blur {
          filter: blur(5px);
        }
      }

  .gen-wrap {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        position: relative;
        .btn {
          margin-bottom: 1rem;
          margin-right: 0;

          &.cancel {
            margin-top: 1rem;
            color: $color-danger;
          }
        }
        .pomView {
          display: none;
          @include respond(large-phone) {
            display: block;
          }
          position: absolute;
          top: -2.5rem;
          left: 4rem;
          h1 {
            color: #111;
            font-size: 18px;
          }
        }
      }

 .form-wrapper {
        width: 50%;
        @include respond(tab-land) {
          width: 100%;
        }
        p {
          margin-bottom: 1rem;
        }
      }

 .msg {
        display: none;
        color: #f23535;
        text-align: center;
        margin-bottom: 10px;
      }




.main .ips__content .gen-wrap .btn {
    margin-bottom: 1rem;
    margin-right: 0;
}

.btn__primary.red {
    color: #f23535;
    margin-top: -.2rem;
}

.btn__primary {
    align-items: center;
    background-color: #fff;
    border: none;
    border-radius: 4px; box-shadow: 0px 4px 5.5px 0px rgba(0, 0, 0, 0.07);
    color: #405c75;
    font-size: 14px;
    font-weight: 500;
    height: 4rem;
    justify-content: center;
    letter-spacing: 1px;
    text-transform: uppercase;
    width: 20rem;
}

.pay-btn{
    padding: 5px 25px;
    width : 100%;
    text-align: center;
    border-radius: 12px;
    font-style: normal;
    font-size: 1rem;
    line-height: normal;
    box-shadow: 0px 4px 5.5px 0px rgba(0, 0, 0, 0.07);
    transition: all 0.2s ease;
    font-family: "RTDromo Medium", "Segoe UI", "Segoe UI Web Regular", "Segoe UI Symbol", "Helvetica Neue", "BBAlpha Sans", "S60 Sans", Arial, sans-serif;
}

.retry-btn{
    padding: 5px 25px;
    text-align: center;
    border-radius: 12px;
    font-style: normal;
    font-size: 1rem;
    line-height: normal;
    box-shadow: 0px 4px 5.5px 0px rgba(0, 0, 0, 0.07);
    transition: all 0.2s ease;
    font-family: "RTDromo Medium", "Segoe UI", "Segoe UI Web Regular", "Segoe UI Symbol", "Helvetica Neue", "BBAlpha Sans", "S60 Sans", Arial, sans-serif;
}

