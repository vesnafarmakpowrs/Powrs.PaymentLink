src: url('../css/Fonts/RTDromo-Book.woff2') format('woff2'),
url('fonts/RTDromo-Book.woff') format('woff');
font-weight: normal;
font-style: normal;
}

@font-face {
font-family: 'RTDromo Book Oblique';
src: url('../css/Fonts/RTDromo-BookOblique.woff2') format('woff2'),
url('fonts/RTDromo-BookOblique.woff') format('woff');
